{
    "manifest_version": 3,
    "name": "Youtube recommend Blocker",
    "version": "1.0",
    "description": "Blocks your YT recommends",
    "permissions": ["activeTab"],
    "content_scripts":[
        {
            "matches":["*://www.youtube.com/*"],
            "js":["content.js"],
            "css":["styles.css"]
        }
    ]

}